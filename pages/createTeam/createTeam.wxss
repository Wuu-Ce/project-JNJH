/* 卡片标题下方横线 */
.card-bottomLine {
  border-bottom: solid 3rpx rgba(0, 0, 0, 0.15);
}

.normal-input {
  position: relative;
  width: 100%;
  height: 68rpx;
  -webkit-overflow-scrolling: touch;
  border-radius: 10rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
}

.solid-normal {
  border: solid 2px rgba(238, 238, 238) !important; 
}
.solid-warnning {
  border: solid 2px rgba(231, 43, 43, 0.5) !important; 
}

.cu-modal {
  background-color: none !important;
}

cu-dialog {
  background-color: rgba(105, 55, 55, 0.808) !important;
}